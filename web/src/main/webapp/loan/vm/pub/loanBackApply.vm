<body>
<form id="form">
    <input type="hidden" id="loanId" name="loanId" value="$!loanId" />
    <input type="hidden" id="module" name="module" value="$!module" />
    <table class="ui-form-fields">
        <tbody>
        <tr>
            <td class="field" width="15%">
                <label class="ui-star">备注：</label>
            </td>
            <td>
                <div class="ui-textarea h80">
                    <input type="hidden" inputId="$!field.propertyName" $!valid />
                    <textarea class="ui-textarea-text" name="note" id="note" maxlength="100"   ></textarea>
                </div>
            </td>
        </tr>
        </tbody>
    </table>
</form>
<hr class="ui-line" />
<div class="ui-layout-closure">
		<span id="btnConfirm" class="ui-closure-button">
			<label class="ui-closure-button-text">$!pageConst.BTN_SURE</label>
		</span>
