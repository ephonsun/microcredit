
#*资产负债存货项明细，存货 编辑界面*#
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<title>存货编辑界面</title>
<link type="text/css" rel="stylesheet" href="../core/css/global.css" />
<link type="text/css" rel="stylesheet" href="../core/css/ui.css" />
<link type="text/css" rel="stylesheet" href="../permission/css/public.css" />
<link type="text/css" rel="stylesheet" href="../uploadify/uploadify.css?v=$!webAppVersion" />
<style type="text/css">
</style>
</head>

<body>
	<form id="form">
        #if($entity)
            <input type="hidden" value="$!entity.loanId" name="loanId" id="loanId">
            <input type="hidden" value="$!entity.loanClassId" name="loanClassId" id="loanClassId">
        #else
            <input type="hidden" value="$!loanId" name="loanId" id="loanId">
            <input type="hidden" value="$!loanClassId" name="loanClassId" id="loanClassId">
        #end
		<table class="ui-form-fields">
			<tbody>
            <input type="hidden" value="$!entity.id" name="id">
			<tr>
                <td class="field" width="5%">
                    <label class="ui-star">类别：</label>
                </td>
                <td width="20%">
                   <div style="width: 80%">
                    <select id="itemType" name="itemType" valid="required">
                        <option></option>
						#datadict("$!entity.itemType","CD_ASSETS_STOCK_TYPE")
                    </select>
                   </div>
                </td>
                <td class="field" width="5%">
                    <label class="ui-star">名称：</label>
                </td>
                <td width="20%">
                    <div class="ui-text divcss9">
                        <input type="text" id="itemName" class="ui-text-text" name="itemName" value="$!entity.itemName" maxlength="30"/>
                    </div>
                </td>
			</tr>

			<tr>
                <td class="field" width="5%">
                    <label>数量：</label>
                </td>
                <td width="20%">
                    <div class="ui-text divcss9">
                        <input type="text" id="itemCount" class="ui-text-text" name="itemCount" value="$!entity.itemCount" maxlength="9"/>
                    </div>
                </td>
                <td class="field" width="5%">
                    <label>单价：</label>
                </td>
                <td width="20%">
                    <div class="ui-text divcss9">
                        <input type="text" id="itemPrice" class="ui-text-text" name="itemPrice" value="$!entity.itemPrice" maxlength="13"/>
                    </div>&nbsp;元
                </td>
			</tr>

			<tr>
				<td class="field" width="5%">
						<label>备注：</label>
				</td>
				<td colspan="3">
					<div class="ui-textarea" style="width: 91.8%">
						<textarea id="remark" name="remark" class="ui-textarea-text" maxlength="100">$!entity.remark</textarea>
					</div>
				</td>
			</tr>
			</tbody>
		</table>
	</form>
    <div style="margin-top: 110px"></div>
    <hr class="ui-line"/>
    <div class="ui-layout-closure divcss10">
        #if($entity)
            <span id="btnUpdate" class="ui-closure-button" onclick="updateStockAssets('$!entity.columnName')">
				<label class="ui-closure-button-text">$!pageConst.BTN_SURE</label>
			</span>
            <span id="btnCancel" class="ui-closure-button ui-closure-button-cancel ml15">
	 		<label class="ui-closure-button-text">$!pageConst.BTN_CANCEL</label>
		    </span>
        #else
            <span id="btnAdd" class="ui-closure-button" onclick="addStockAssets('$!columnName')">
				<label class="ui-closure-button-text">$!pageConst.BTN_SURE</label>
			    </span>
            <span id="btnCancel" class="ui-closure-button ui-closure-button-cancel ml15">
                    <label class="ui-closure-button-text">$!pageConst.BTN_CANCEL</label>
                </span>
        #end
    </div>
	<script type="text/javascript" src="../core/js/third/jQuery/jquery-1.8.3.min.js?v=$!webAppVersion"></script>
	<script type="text/javascript" src="../core/js/banger/core/banger.core.js?v=$!webAppVersion"></script>
	<script type="text/javascript" src="../core/js/banger/ui/banger.ui.js?v=$!webAppVersion"></script>
	<script type="text/javascript" src="../core/js/banger/ui/tools/banger.form.js?v=$!webAppVersion"></script>
	<script type="text/javascript" src="../core/js/banger/ui/controls/banger.datepicker.js?v=$!webAppVersion"></script>
	<script type="text/javascript" src="../core/js/banger/ui/controls/banger.flexigrid.js?v=$!webAppVersion"></script>
	<script type="text/javascript" src="../core/js/banger/ui/controls/banger.selectbox.js?v=$!webAppVersion"></script>
	<script type="text/javascript" src="../core/js/banger/ui/tools/banger.dialog.js?v=$!webAppVersion"></script>
	<script type="text/javascript" src="../core/js/banger/ui/tools/banger.validator.js?v=$!webAppVersion"></script>
	<script type="text/javascript" src="../biz/js/banger.biz.js?v=$!webAppVersion"></script>
	<!-- //附件上传相关脚本 -->
	<script type="text/javascript" src="../uploadify/swfobject.js?v=$!webAppVersion"></script>
	<script type="text/javascript" src="../uploadify/jquery.uploadify.v2.1.4.min.js?v=$!webAppVersion"></script>
	<!-- //初始化脚本 -->
	<script type="text/javascript" src="../biz/js/banger.layout.js"></script>
	<!-- //页面业务逻辑脚本 -->
    <script type="text/javascript" src="../loan/js/assets/assetsStockSave.js"></script>
</body>
</html>
