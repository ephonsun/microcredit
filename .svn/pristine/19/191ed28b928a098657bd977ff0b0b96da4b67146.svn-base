@charset "utf-8";/*======================================== 描述：系统主界面样式 作者：丁伟 2013.11.14 修改：========================================*/html { display:block; height:100%; overflow:hidden; }body { display:block; height:100%; background-color:#fff; padding:0 !important; overflow:hidden; color:#3d3d3d; font:12px/1 'microsoft yahei'; }/* head *//* .head { position:relative; height:60px; background:#50b3e2 url(../imgs/bg.gif) no-repeat 0 0; overflow:hidden; } */.head { position:relative; height:60px; background:#50b3e2 url(../imgs/bg.gif) no-repeat 0 0; }.head1024 { position:relative; height:60px; background:#50b3e2 url(../imgs/bgl.gif) no-repeat 0 0; }.logo, .logo img, .logo span { float:left; }.logo { display:inline; height:32px; margin:14px 0 0 10px; overflow:hidden; color:#fff; }.logo img { margin-right:10px; }.logo span { margin-top:16px; font-size:12px; }.nav { position:absolute; top:34px; left:540px; width:1000px; color:#fff; }.nav li { float:left; height:20px; overflow:hidden; line-height:20px; line-height:22px \9; }.nav li.separator { width:5px; background:url(../imgs/bg.gif) no-repeat 2px -60px; margin:0 7px; }.nav .logout { color:#fffe98; }.nav li span { padding: 5px; }.nav li span.role i.ddl { background: url(../imgs/ddl.gif) no-repeat center center; margin-left: 5px; padding: 2px; overflow: hidden; }/* body */.body { position:absolute; top:60px; bottom:0; left:0; right:0; width:100%; height:auto; -height:100%; background:#fff url(../imgs/body.gif) repeat 0 0; overflow:hidden; }.body-side { position:absolute; top:0; left:0; width:180px; height:100%; border-right:1px solid #b9c7dd; background-color:#f1f4f8; }.menu-toggle { position:absolute; top:200px; right:-11px; cursor:pointer; display:block; width:10px; height:100px; background:url(../imgs/bg.gif) no-repeat -20px -65px; }.menu-toggle-on { background-position:-30px -65px; }.menu-head { height:34px; border-bottom:1px solid #b9c7dd; background:#e8eef7 url(../imgs/dt.gif) repeat-x 0 0; padding:0 10px; overflow:hidden; line-height:34px; line-height:36px \9; font-weight:700; }.menu-body { position:absolute; top:35px; left:0; width:100%; overflow:auto; }.menu-body dl {  }.menu-body dt { height:34px; border-bottom:1px solid #b9c7dd; background:#e8eef7 url(../imgs/dt.gif) repeat-x 0 0; padding:0 10px; overflow:hidden; line-height:34px; line-height:36px \9; }.menu-body dt i { float:right; width:9px; height:6px; background:url(../imgs/bg.gif) no-repeat -6px -65px; margin-top:14px; overflow:hidden; }.menu-body dt span { cursor:default; float:left; width:124px; overflow:hidden; }.menu-body dt.on { font-weight:700; }.menu-body dt.on i { background-position:-6px -71px; }.menu-body dd { display:none; }.menu-body dd.on { display:block; border-bottom:1px solid #b9c7dd; }.menu-body ul { border:10px solid #fff; }.menu-body li { height:30px; border-bottom:1px solid #fff; background-color:#e8eef7; overflow:hidden; padding-left:25px; line-height:30px; line-height:32px \9; }.menu-body li.hover, .menu-body li:hover { background-color:#cee1f2; }.menu-body li.on { background:#5daee1 url(../imgs/bg.gif) no-repeat -152px -69px; color:#fff; }.menu-body li span { cursor:default; }.body-main { height:100%; margin-left:191px; overflow:hidden; }/* tabs */.tabs { padding-top:10px; }.tabs-head { position:relative; height:32px; }.tabs-items { position:relative; height:32px; margin-right:110px; overflow:hidden; }.tabs-items ul { position:absolute; min-width:180px; height:32px; }.tabs-items li.tabs-item { position:relative; z-index:1; float:left; width:178px; height:30px; border-width:1px 1px 0 1px; border-style:solid; border-color:#b6c4d8; background-color:#eff4fc; margin-right:-1px; overflow:hidden; }.tabs-items li.hover, .tabs-items li:hover { z-index:2; border-color:#378ebd; background-color:#6cc3ed; }.tabs-items li.on { z-index:3; height:31px; border-width:1px 1px 0 1px; border-color:#378ebd; background-color:#48aadd; }.tabs-items h3.tabs-item-title { cursor:default; float:left; display:inline; height:30px; width:140px; margin-left:10px; overflow:hidden; white-space:nowrap; -o-text-overflow:ellipsis; text-overflow:ellipsis; line-height:30px; line-height:32px \9; }.tabs-items li.hover h3.tabs-item-title, .tabs-items li:hover h3.tabs-item-title, .tabs-items li.on h3.tabs-item-title { color:#fff; }.tabs-items b.tabs-item-close { position:absolute; top:7px; right:5px; width:17px; height:17px; background:url(../imgs/bg.gif) no-repeat -45px -65px;  }.tabs-items b.tabs-item-close:hover, .tabs-items b.tabs-item-close.hover { background-position: -45px -82px; }.tabs-action { position:absolute; top:0; right:0; height:32px; padding-right:4px; }.tabs-action b { float:left; width:20px; height:20px; background-image:url(../imgs/bg.gif); background-repeat:no-repeat; margin:6px 0 0 4px; overflow:hidden; }.tabs-btn-right { background-position:-67px -65px; }.tabs-btn-right:hover { background-position:-67px -85px; }.tabs-btn-left { background-position:-87px -65px; }.tabs-btn-left:hover { background-position:-87px -85px; }.tabs-btn-fav { background-position:-107px -65px; }.tabs-btn-fav:hover { background-position:-107px -85px; }.tabs-btn-close { background-position:-127px -65px; }.tabs-btn-close:hover { background-position:-127px -85px; }.tabs-fav { position:absolute; top:34px; right:0; display:none; width:200px; border:1px solid #378ebd; background-color:#fff; overflow:auto; }.tabs-fav ul {  }.tabs-fav li { padding:0 10px; overflow:hidden; }.tabs-fav li.hover, .tabs-fav li:hover { background-color:#cee1f2; }.tabs-fav h3 { cursor:default; width:100%; height:30px; overflow:hidden; white-space:nowrap; text-overflow:ellipsis; line-height:30px; line-height:32px \9; }.tabs-line { height:2px; border-width:1px 0 1px 1px; border-style:solid; border-color:#378ebd; background-color:#48aadd; margin-top:-1px; overflow:hidden; }.tabs-body { height:1000px; border-left:1px solid #b9c7dd; background-color:#f1f4f8; overflow:visible; }.tabs-body-child { display:none; width:100%; height:100%; overflow:hidden; }.tabs-body-child iframe { width:100%; height:1000px; }/* 系统闲置时间过长、异地登陆等弹出层 */.dialog-abnormal { padding:20px 30px; }.dialog-abnormal-tips { background:url(../imgs/warning.gif) no-repeat 0 0; padding:10px 0px 10px 60px; line-height:2; font-size:14px; font-family:'microsoft yahei'; }.dialog-abnormal-error { height:16px; margin-top:10px; padding-left:38px; line-height:16px; overflow:hidden; color:#f00; font-size:14px; }.dialog-abnormal-field { margin-top:10px; }.dialog-abnormal-field .label { margin-right:10px; vertical-align:middle; font-size:14px; }.dialog-abnormal-field .text { width:220px; height:28px; border:1px solid #b6c4d8; margin:0; padding:0px 5px; vertical-align:middle; line-height:2; font-size:14px; }/* dial */div.dial { padding: 8px 0px 8px 8px; font-size: 0; }div.dial .ml { margin-left: 5px; }div.dial .middle { vertical-align: middle; }div.dial .text { border: 1px solid #b1b1b1; background-color: #fff; width: 96px; height: 24px;font-size:12px;line-height: 24px; padding: 0px 2px; }div.dial .button { cursor: pointer; width: 26px; height: 26px; border: 0; }div.dial .bg1 { background: url(../imgs/dial.jpg) no-repeat center 0px; }div.dial .bg2 { background: url(../imgs/dial.jpg) no-repeat center -26px; }