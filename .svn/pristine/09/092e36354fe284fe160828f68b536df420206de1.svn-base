  <title>页面模板列表</title>
  <author>zhusw</author>
  <view>/biz/vm/queryPageList.vm</view>
  <extraView>/biz/vm/addQueryPageList.vm</extraView>
  <url>../build/getAutoPageList.html</url>
  <sqlId>queryAutoBuildPageListData</sqlId>
  <tableName>AUTO_PAGE_LIST</tableName>
  <sqlEnable>true</sqlEnable>
  <sqlString>&lt;sql id=&apos;queryAutoBuildPageListData&apos; &gt;
	SELECT * FROM AUTO_PAGE_LIST 
	&lt;where&gt;
		&lt;clause prepend=&apos;and&apos; assert=&apos;!isNullOrEmpty([pageId])&apos; &gt; PAGE_ID like &apos;%[pageId]%&apos; &lt;/clause&gt;
		&lt;clause prepend=&apos;and&apos; assert=&apos;!isNullOrEmpty([pageName])&apos; &gt; PAGE_NAME like &apos;%[pageName]%&apos; &lt;/clause&gt;
		&lt;clause prepend=&apos;and&apos; assert=&apos;!isNullOrEmpty([pageView])&apos; &gt; PAGE_VIEW like &apos;%[pageView]%&apos; &lt;/clause&gt;
		&lt;clause prepend=&apos;and&apos; assert=&apos;!isNullOrEmpty([pageAuthor])&apos; &gt; PAGE_AUTHOR = &apos;[pageAuthor]&apos; &lt;/clause&gt;
		&lt;clause parent=&apos;DateSpan&apos; params=&apos;PAGE_UPDATE_DATE,pageUpdateDate,pageUpdateDateEnd&apos; &gt;&lt;/clause&gt;
	&lt;/where&gt;
&lt;/sql&gt;</sqlString>
