package banger.domain.config;

import java.io.Serializable;
import java.util.Date;

/**
 * 自定义字段表
 */
public class AutoTableColumn implements Serializable {
	private static final long serialVersionUID = 5417608609312520289L;
	private Integer fieldId;					//主键ID
	private Integer tableId;					//表ID
	private String tableName;					//表名
	private String fieldName;					//字段名称
	private String fieldType;					//字段类型
	private String fieldNumberUnit;					//字段数量单位
	private String fieldConstraintRule;					//字段校验规则（为空不校验）
	private String fieldColumn;					//字段列名
	private String fieldColumnDisplay;					//字段列描述
	private Integer fieldAppIsShow;					//APP端是否可显示默是显示的
	private Integer fieldWebIsShow;					//web端是否可显示
	private Integer fieldIsQuery;					//是否可查询默认不作为条件
	private Integer fieldIsRequired;					//是否必填
	private Integer fieldSortno;					//字段排序
	private String fieldDictName;					//数据字典
	private Integer fieldLength;					//字段可输入长度
	private Integer isActived;					//是否启用0 禁用1 启用
	private Integer isFixed;					//是否为内置
	private Date createDate;					//创建时间
	private Date updateDate;					//更新时间
	private Integer createUser;					//创建用户
	private Integer updateUser;					//更新用户
	private String columnModule;
	protected String popupUrl;		//弹出对话框地址
	private String defaultValue;				//默认值
	public String getColumnModule() {
		return columnModule;
	}

	public void setColumnModule(String columnModule) {
		this.columnModule = columnModule;
	}

	public Integer getFieldId(){
		return this.fieldId;
	}

	public void setFieldId(Integer fieldId){
		this.fieldId=fieldId;
	}

	public Integer getTableId(){
		return this.tableId;
	}

	public void setTableId(Integer tableId){
		this.tableId=tableId;
	}

	public String getTableName(){
		return this.tableName;
	}

	public void setTableName(String tableName){
		this.tableName=tableName;
	}

	public String getFieldName(){
		return this.fieldName;
	}

	public void setFieldName(String fieldName){
		this.fieldName=fieldName;
	}

	public String getFieldType(){
		return this.fieldType;
	}

	public void setFieldType(String fieldType){
		this.fieldType=fieldType;
	}

	public String getFieldNumberUnit(){
		return this.fieldNumberUnit;
	}

	public void setFieldNumberUnit(String fieldNumberUnit){
		this.fieldNumberUnit=fieldNumberUnit;
	}

	public String getFieldConstraintRule(){
		return this.fieldConstraintRule;
	}

	public void setFieldConstraintRule(String fieldConstraintRule){
		this.fieldConstraintRule=fieldConstraintRule;
	}

	public String getFieldColumn(){
		return this.fieldColumn;
	}

	public void setFieldColumn(String fieldColumn){
		this.fieldColumn=fieldColumn;
	}

	public String getFieldColumnDisplay(){
		return this.fieldColumnDisplay;
	}

	public void setFieldColumnDisplay(String fieldColumnDisplay){
		this.fieldColumnDisplay=fieldColumnDisplay;
	}

	public Integer getFieldAppIsShow(){
		return this.fieldAppIsShow;
	}

	public void setFieldAppIsShow(Integer fieldAppIsShow){
		this.fieldAppIsShow=fieldAppIsShow;
	}

	public Integer getFieldWebIsShow(){
		return this.fieldWebIsShow;
	}

	public void setFieldWebIsShow(Integer fieldWebIsShow){
		this.fieldWebIsShow=fieldWebIsShow;
	}

	public Integer getFieldIsQuery(){
		return this.fieldIsQuery;
	}

	public void setFieldIsQuery(Integer fieldIsQuery){
		this.fieldIsQuery=fieldIsQuery;
	}

	public Integer getFieldIsRequired(){
		return this.fieldIsRequired;
	}

	public void setFieldIsRequired(Integer fieldIsRequired){
		this.fieldIsRequired=fieldIsRequired;
	}

	public Integer getFieldSortno(){
		return this.fieldSortno;
	}

	public void setFieldSortno(Integer fieldSortno){
		this.fieldSortno=fieldSortno;
	}

	public String getFieldDictName(){
		return this.fieldDictName;
	}

	public void setFieldDictName(String fieldDictName){
		this.fieldDictName=fieldDictName;
	}

	public Integer getFieldLength(){
		return this.fieldLength;
	}

	public void setFieldLength(Integer fieldLength){
		this.fieldLength=fieldLength;
	}

	public Integer getIsActived(){
		return this.isActived;
	}

	public void setIsActived(Integer isActived){
		this.isActived=isActived;
	}

	public Integer getIsFixed(){
		return this.isFixed;
	}

	public void setIsFixed(Integer isFixed){
		this.isFixed=isFixed;
	}

	public Date getCreateDate(){
		return this.createDate;
	}

	public void setCreateDate(Date createDate){
		this.createDate=createDate;
	}

	public Date getUpdateDate(){
		return this.updateDate;
	}

	public void setUpdateDate(Date updateDate){
		this.updateDate=updateDate;
	}

	public Integer getCreateUser(){
		return this.createUser;
	}

	public void setCreateUser(Integer createUser){
		this.createUser=createUser;
	}

	public Integer getUpdateUser(){
		return this.updateUser;
	}

	public void setUpdateUser(Integer updateUser){
		this.updateUser=updateUser;
	}

	public String getPopupUrl() {
		return popupUrl;
	}

	public void setPopupUrl(String popupUrl) {
		this.popupUrl = popupUrl;
	}

	public String getDefaultValue() {
		return defaultValue;
	}

	public void setDefaultValue(String defaultValue) {
		this.defaultValue = defaultValue;
	}
}
