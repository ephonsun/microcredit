package banger.domain.loan;

import java.io.Serializable;
import java.util.Date;
import java.math.BigDecimal;

import banger.framework.entity.Entity;

/**
 * 交叉检验收入表
 */
public class LoanCrossCheckIncome implements Serializable {
	private static final long serialVersionUID = 2221567500929499649L;
	private Integer id;					//主键ID
	private Integer loanId;					//贷款ID
	private Integer loanClassId;					//贷款分类ID1 经营类2 消费类
	private BigDecimal wageFlow;					//工资流水
	private BigDecimal wageFlowDeviation;					//偏差率（工资流水）
	private BigDecimal incomeCert;					//收入证明
	private BigDecimal incomeCertDeviation;					//偏差率（收入证明）
	private BigDecimal personTax;					//个人税单
	private BigDecimal personTaxDeviation;					//偏差率（个人税单）
	private BigDecimal otherIncome;					//其他
	private BigDecimal otherIncomeDeviation;					//偏差率（其他）
	private String infomation;					//说明
	private Date createDate;					//创建时间
	private Date updateDate;					//更新时间
	private Integer createUser;					//创建用户
	private Integer updateUser;					//更新用户

	public Integer getId(){
		return this.id;
	}

	public void setId(Integer id){
		this.id=id;
	}

	public Integer getLoanId(){
		return this.loanId;
	}

	public void setLoanId(Integer loanId){
		this.loanId=loanId;
	}

	public Integer getLoanClassId(){
		return this.loanClassId;
	}

	public void setLoanClassId(Integer loanClassId){
		this.loanClassId=loanClassId;
	}

	public BigDecimal getWageFlow(){
		return this.wageFlow;
	}

	public void setWageFlow(BigDecimal wageFlow){
		this.wageFlow=wageFlow;
	}

	public BigDecimal getWageFlowDeviation(){
		return this.wageFlowDeviation;
	}

	public void setWageFlowDeviation(BigDecimal wageFlowDeviation){
		this.wageFlowDeviation=wageFlowDeviation;
	}

	public BigDecimal getIncomeCert(){
		return this.incomeCert;
	}

	public void setIncomeCert(BigDecimal incomeCert){
		this.incomeCert=incomeCert;
	}

	public BigDecimal getIncomeCertDeviation(){
		return this.incomeCertDeviation;
	}

	public void setIncomeCertDeviation(BigDecimal incomeCertDeviation){
		this.incomeCertDeviation=incomeCertDeviation;
	}

	public BigDecimal getPersonTax(){
		return this.personTax;
	}

	public void setPersonTax(BigDecimal personTax){
		this.personTax=personTax;
	}

	public BigDecimal getPersonTaxDeviation(){
		return this.personTaxDeviation;
	}

	public void setPersonTaxDeviation(BigDecimal personTaxDeviation){
		this.personTaxDeviation=personTaxDeviation;
	}

	public BigDecimal getOtherIncome(){
		return this.otherIncome;
	}

	public void setOtherIncome(BigDecimal otherIncome){
		this.otherIncome=otherIncome;
	}

	public BigDecimal getOtherIncomeDeviation(){
		return this.otherIncomeDeviation;
	}

	public void setOtherIncomeDeviation(BigDecimal otherIncomeDeviation){
		this.otherIncomeDeviation=otherIncomeDeviation;
	}

	public String getInfomation(){
		return this.infomation;
	}

	public void setInfomation(String infomation){
		this.infomation=infomation;
	}

	public Date getCreateDate(){
		return this.createDate;
	}

	public void setCreateDate(Date createDate){
		this.createDate=createDate;
	}

	public Date getUpdateDate(){
		return this.updateDate;
	}

	public void setUpdateDate(Date updateDate){
		this.updateDate=updateDate;
	}

	public Integer getCreateUser(){
		return this.createUser;
	}

	public void setCreateUser(Integer createUser){
		this.createUser=createUser;
	}

	public Integer getUpdateUser(){
		return this.updateUser;
	}

	public void setUpdateUser(Integer updateUser){
		this.updateUser=updateUser;
	}

}
