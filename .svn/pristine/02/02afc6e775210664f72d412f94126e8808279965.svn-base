package banger.domain.loan;

import java.io.Serializable;
import java.util.Date;

import banger.framework.codetable.CodeTable;
import banger.framework.entity.Entity;

/**
 * 贷款监控信息表
 */
public class LoanMonitorInfo implements Serializable {
	private static final long serialVersionUID = 3497851710593617391L;
	private Integer id;					//主键
	private Integer loanId;					//贷款ID
	private Date loanMonitorDate;					//监控日期
	private String loanMonitorType;					//监控类型
	private Integer loanMonitorUserId;					//监控人用户ID
	private Date loanCompleteDate;					//监控完成日期
	private String loanRevisitType;					//回访类型
	private String loanResultContent;					//回访结果
	private String loanMonitorState;					//监控状态
	private Date createDate;					//创建时间
	private Date updateDate;					//更新时间
	private Integer createUser;					//创建用户
	private Integer updateUser;					//更新用户

	public Integer getId(){
		return this.id;
	}

	public void setId(Integer id){
		this.id=id;
	}

	public Integer getLoanId(){
		return this.loanId;
	}

	public void setLoanId(Integer loanId){
		this.loanId=loanId;
	}

	public Date getLoanMonitorDate(){
		return this.loanMonitorDate;
	}

	public void setLoanMonitorDate(Date loanMonitorDate){
		this.loanMonitorDate=loanMonitorDate;
	}

	public String getLoanMonitorType(){
		return this.loanMonitorType;
	}

	public void setLoanMonitorType(String loanMonitorType){
		this.loanMonitorType=loanMonitorType;
	}

	public Integer getLoanMonitorUserId(){
		return this.loanMonitorUserId;
	}

	public void setLoanMonitorUserId(Integer loanMonitorUserId){
		this.loanMonitorUserId=loanMonitorUserId;
	}

	public Date getLoanCompleteDate(){
		return this.loanCompleteDate;
	}

	public void setLoanCompleteDate(Date loanCompleteDate){
		this.loanCompleteDate=loanCompleteDate;
	}

	public String getLoanRevisitType(){
		return this.loanRevisitType;
	}

	public void setLoanRevisitType(String loanRevisitType){
		this.loanRevisitType=loanRevisitType;
	}

	public String getLoanResultContent(){
		return this.loanResultContent;
	}

	public void setLoanResultContent(String loanResultContent){
		this.loanResultContent=loanResultContent;
	}

	public String getLoanMonitorState(){
		return this.loanMonitorState;
	}

	public void setLoanMonitorState(String loanMonitorState){
		this.loanMonitorState=loanMonitorState;
	}

	public Date getCreateDate(){
		return this.createDate;
	}

	public void setCreateDate(Date createDate){
		this.createDate=createDate;
	}

	public Date getUpdateDate(){
		return this.updateDate;
	}

	public void setUpdateDate(Date updateDate){
		this.updateDate=updateDate;
	}

	public Integer getCreateUser(){
		return this.createUser;
	}

	public void setCreateUser(Integer createUser){
		this.createUser=createUser;
	}

	public Integer getUpdateUser(){
		return this.updateUser;
	}

	public void setUpdateUser(Integer updateUser){
		this.updateUser=updateUser;
	}

}
