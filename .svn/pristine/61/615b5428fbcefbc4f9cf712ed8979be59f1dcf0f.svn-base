package banger.domain.product;

import java.io.Serializable;
import java.util.Date;

/**
 * 产品列表
 */
public class ProdProduct implements Serializable {
	private static final long serialVersionUID = 1069090576610252756L;
	private Integer productId;					//产品列表ID
	private Integer productCreateUser;					//
	private Integer productIsdel;					//1 删除0 未删除
	private String productInfo;					//产品介绍
	private String productCode;					//产品代码
	private String productName;					//产品名称
	private Date productCreateDate;					//
	private Integer productUpdateUser;					//
	private Date productUpdateDate;					//
	private String productType;					//产品类型
	private Integer loanType;					//贷款类型

	public Integer getProductId(){
		return this.productId;
	}

	public void setProductId(Integer productId){
		this.productId=productId;
	}

	public Integer getProductCreateUser(){
		return this.productCreateUser;
	}

	public void setProductCreateUser(Integer productCreateUser){
		this.productCreateUser=productCreateUser;
	}

	public Integer getProductIsdel(){
		return this.productIsdel;
	}

	public void setProductIsdel(Integer productIsdel){
		this.productIsdel=productIsdel;
	}

	public String getProductInfo(){
		return this.productInfo;
	}

	public void setProductInfo(String productInfo){
		this.productInfo=productInfo;
	}

	public String getProductCode(){
		return this.productCode;
	}

	public void setProductCode(String productCode){
		this.productCode=productCode;
	}

	public String getProductName(){
		return this.productName;
	}

	public void setProductName(String productName){
		this.productName=productName;
	}

	public Date getProductCreateDate(){
		return this.productCreateDate;
	}

	public void setProductCreateDate(Date productCreateDate){
		this.productCreateDate=productCreateDate;
	}

	public Integer getProductUpdateUser(){
		return this.productUpdateUser;
	}

	public void setProductUpdateUser(Integer productUpdateUser){
		this.productUpdateUser=productUpdateUser;
	}

	public Date getProductUpdateDate(){
		return this.productUpdateDate;
	}

	public void setProductUpdateDate(Date productUpdateDate){
		this.productUpdateDate=productUpdateDate;
	}

	public String getProductType(){
		return this.productType;
	}

	public void setProductType(String productType){
		this.productType=productType;
	}

	public Integer getLoanType() {
		return loanType;
	}

	public void setLoanType(Integer loanType) {
		this.loanType = loanType;
	}
}
