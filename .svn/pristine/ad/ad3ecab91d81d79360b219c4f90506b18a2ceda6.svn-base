package banger.domain.loan.tmp;

import java.io.Serializable;
import java.math.BigDecimal;

import banger.framework.entity.Entity;

/**
 * 贷款信息核实接口
 */
public class TmpLoanInfo implements Serializable {
	private static final long serialVersionUID = 7879557928327482146L;
	private String loanAccount;					//贷款帐号
	private String contractNo;					//贷款合同号
	private String repaymentAccount;					//还款账号
	private String incomeAccount;					//入账账号
	private String iouNo;					//借据编号
	private String contractCnNo;					//中文合同编号
	private Integer limitYear;					//期限（月）
	private String beginDate;					//起始日期
	private String endDate;					//终止日期
	private BigDecimal loanMoney;					//合同金额

	public String getLoanAccount(){
		return this.loanAccount;
	}

	public void setLoanAccount(String loanAccount){
		this.loanAccount=loanAccount;
	}

	public String getContractNo(){
		return this.contractNo;
	}

	public void setContractNo(String contractNo){
		this.contractNo=contractNo;
	}

	public String getRepaymentAccount(){
		return this.repaymentAccount;
	}

	public void setRepaymentAccount(String repaymentAccount){
		this.repaymentAccount=repaymentAccount;
	}

	public String getIncomeAccount(){
		return this.incomeAccount;
	}

	public void setIncomeAccount(String incomeAccount){
		this.incomeAccount=incomeAccount;
	}

	public String getIouNo(){
		return this.iouNo;
	}

	public void setIouNo(String iouNo){
		this.iouNo=iouNo;
	}

	public String getContractCnNo(){
		return this.contractCnNo;
	}

	public void setContractCnNo(String contractCnNo){
		this.contractCnNo=contractCnNo;
	}

	public Integer getLimitYear(){
		return this.limitYear;
	}

	public void setLimitYear(Integer limitYear){
		this.limitYear=limitYear;
	}

	public String getBeginDate(){
		return this.beginDate;
	}

	public void setBeginDate(String beginDate){
		this.beginDate=beginDate;
	}

	public String getEndDate(){
		return this.endDate;
	}

	public void setEndDate(String endDate){
		this.endDate=endDate;
	}

	public BigDecimal getLoanMoney(){
		return this.loanMoney;
	}

	public void setLoanMoney(BigDecimal loanMoney){
		this.loanMoney=loanMoney;
	}

}
