package banger.domain.system;

import java.io.Serializable;
import java.util.Date;

/**
 * Created by Administrator on 2017/4/18.
 */
public class SysBasicConfig implements Serializable {

    private static final long serialVersionUID = 7826462475206910867L;
    //配置项ID
    private Integer basicConfigId;
    //配置项名称
    private String basicConfigName;
    //配置项KEY
    private String basicConfigKey;
    //配置项状态（1开启，2停用）
    private String configStatus;
    //配置项值
    private Integer configValue;
    //配置项类别（1、基础数据，2、配置开关，3、接口开关）
    private Integer configType;
    //是否已删除（0未删除，1已删除）
    private Integer isDel;
    //创建用户ID
    private Integer createUser;
    //创建时间
    private Date createDate;
    //修改用户ID
    private Integer updateUser;
    //修改时间
    private Date updateDate;
    //备注
    private String remarks;

    public Integer getConfigType() {
        return configType;
    }

    public void setConfigType(Integer configType) {
        this.configType = configType;
    }

    public Integer getBasicConfigId() {
        return basicConfigId;
    }

    public void setBasicConfigId(Integer basicConfigId) {
        this.basicConfigId = basicConfigId;
    }

    public String getBasicConfigName() {
        return basicConfigName;
    }

    public void setBasicConfigName(String basicConfigName) {
        this.basicConfigName = basicConfigName;
    }

    public String getBasicConfigKey() {
        return basicConfigKey;
    }

    public void setBasicConfigKey(String basicConfigKey) {
        this.basicConfigKey = basicConfigKey;
    }

    public String getConfigStatus() {
        return configStatus;
    }

    public void setConfigStatus(String configStatus) {
        this.configStatus = configStatus;
    }

    public Integer getConfigValue() {
        return configValue;
    }

    public void setConfigValue(Integer configValue) {
        this.configValue = configValue;
    }

    public Integer getIsDel() {
        return isDel;
    }

    public void setIsDel(Integer isDel) {
        this.isDel = isDel;
    }

    public Integer getCreateUser() {
        return createUser;
    }

    public void setCreateUser(Integer createUser) {
        this.createUser = createUser;
    }

    public Date getCreateDate() {
        return createDate;
    }

    public void setCreateDate(Date createDate) {
        this.createDate = createDate;
    }

    public Integer getUpdateUser() {
        return updateUser;
    }

    public void setUpdateUser(Integer updateUser) {
        this.updateUser = updateUser;
    }

    public Date getUpdateDate() {
        return updateDate;
    }

    public void setUpdateDate(Date updateDate) {
        this.updateDate = updateDate;
    }

    public String getRemarks() {
        return remarks;
    }

    public void setRemarks(String remarks) {
        this.remarks = remarks;
    }
}
