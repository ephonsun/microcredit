package banger.domain.loan;

import java.io.Serializable;
import java.util.Date;

import banger.framework.entity.Entity;

/**
 * 贷款审计表
 */
public class LoanAudit implements Serializable {
	private static final long serialVersionUID = 1227807181674895846L;
	private Integer loanAuditId;					//审计ID
	private Integer loanId;					//贷款ID
	private Integer loanAuditState;					//审计状态
	private String loanAuditors;					//审计员
	private String loanAuditRemark;					//
	private Integer createUser;					//创建人
	private Date createDate;					//创建时间
	private Integer updateUser;					//修改人
	private Date updateDate;					//修改时间

	public Integer getLoanAuditId(){
		return this.loanAuditId;
	}

	public void setLoanAuditId(Integer loanAuditId){
		this.loanAuditId=loanAuditId;
	}

	public Integer getLoanId(){
		return this.loanId;
	}

	public void setLoanId(Integer loanId){
		this.loanId=loanId;
	}

	public Integer getLoanAuditState(){
		return this.loanAuditState;
	}

	public void setLoanAuditState(Integer loanAuditState){
		this.loanAuditState=loanAuditState;
	}

	public String getLoanAuditors(){
		return this.loanAuditors;
	}

	public void setLoanAuditors(String loanAuditors){
		this.loanAuditors=loanAuditors;
	}

	public String getLoanAuditRemark(){
		return this.loanAuditRemark;
	}

	public void setLoanAuditRemark(String loanAuditRemark){
		this.loanAuditRemark=loanAuditRemark;
	}

	public Integer getCreateUser(){
		return this.createUser;
	}

	public void setCreateUser(Integer createUser){
		this.createUser=createUser;
	}

	public Date getCreateDate(){
		return this.createDate;
	}

	public void setCreateDate(Date createDate){
		this.createDate=createDate;
	}

	public Integer getUpdateUser(){
		return this.updateUser;
	}

	public void setUpdateUser(Integer updateUser){
		this.updateUser=updateUser;
	}

	public Date getUpdateDate(){
		return this.updateDate;
	}

	public void setUpdateDate(Date updateDate){
		this.updateDate=updateDate;
	}

}
