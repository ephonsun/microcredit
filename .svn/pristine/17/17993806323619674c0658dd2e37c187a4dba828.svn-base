package banger.domain.layout;

import banger.framework.pagesize.PageSize;

public class QueryPageSize extends PageSize {
	
	public int getPage() {
		return pageNum;
	}
	public void setPage(int page) {
		this.pageNum = page;
	}
	public int getSize() {
		return pageSize;
	}
	public void setSize(int size) {
		this.pageSize = size;
	}
	public int getCount() {
		return total;
	}
	public void setCount(int count) {
		this.total = count;
	}
	
	
}
