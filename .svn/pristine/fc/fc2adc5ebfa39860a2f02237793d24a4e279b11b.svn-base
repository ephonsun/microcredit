package banger.framework.web.dojo;

public class DojoConvertException extends RuntimeException {
	private static final long serialVersionUID = -6749588763984065128L;

	public DojoConvertException(String message){
		super("Dojo "+message);
	}
	
	public DojoConvertException(String message,Throwable cause){
		super("Dojo "+message,cause);
	}
}
