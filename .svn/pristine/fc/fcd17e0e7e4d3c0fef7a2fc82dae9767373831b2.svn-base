package banger.domain.loan;

import java.io.Serializable;
import java.math.BigDecimal;

import banger.framework.entity.Entity;

/**
 * 损益情况经营类收入支出明细月份项
 */
public class LoanProfitBizIncomeMonth implements Serializable {
	private static final long serialVersionUID = 2033182519834844612L;
	private Integer id;					//主键ID
	private Integer incomeId;					//收支表ID
	private Integer yearVal;					//年份
	private Integer monthVal;					//月份
	private BigDecimal monthAmount;					//金额

	public Integer getId(){
		return this.id;
	}

	public void setId(Integer id){
		this.id=id;
	}

	public Integer getIncomeId(){
		return this.incomeId;
	}

	public void setIncomeId(Integer incomeId){
		this.incomeId=incomeId;
	}

	public Integer getYearVal(){
		return this.yearVal;
	}

	public void setYearVal(Integer yearVal){
		this.yearVal=yearVal;
	}

	public Integer getMonthVal(){
		return this.monthVal;
	}

	public void setMonthVal(Integer monthVal){
		this.monthVal=monthVal;
	}

	public BigDecimal getMonthAmount(){
		return this.monthAmount;
	}

	public void setMonthAmount(BigDecimal monthAmount){
		this.monthAmount=monthAmount;
	}

}
