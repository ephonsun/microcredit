package banger.domain.loan.finance;

import java.math.BigDecimal;

/**
 * Created by Administrator on 2017/8/21.
 */
public class ConsumeBreakEvenAnalysis {

    //    家庭收入		来源于资产负债表填入的信息
    private BigDecimal householdIncome;
    //    其他收入
    private BigDecimal otherIncome;
    //    固定支出
    private BigDecimal fixedExpenditure;
    //    个人所得税
    private BigDecimal individualIncomeTax;
    //    其他支出
    private BigDecimal otherExpenditure;
    //    总收入	总收入=损益表家庭收入
    private BigDecimal grossIncome;
    //    月平均月可支	月平均月可支=(家庭收入+其他收入-固定支出-个人所得税-其他支出)/月份数（四舍五入，不保留小数）
    private BigDecimal monthlyAverageAvailable;

    public BigDecimal getHouseholdIncome() {
        return householdIncome;
    }

    public void setHouseholdIncome(BigDecimal householdIncome) {
        this.householdIncome = householdIncome;
    }

    public BigDecimal getOtherIncome() {
        return otherIncome;
    }

    public void setOtherIncome(BigDecimal otherIncome) {
        this.otherIncome = otherIncome;
    }

    public BigDecimal getFixedExpenditure() {
        return fixedExpenditure;
    }

    public void setFixedExpenditure(BigDecimal fixedExpenditure) {
        this.fixedExpenditure = fixedExpenditure;
    }

    public BigDecimal getIndividualIncomeTax() {
        return individualIncomeTax;
    }

    public void setIndividualIncomeTax(BigDecimal individualIncomeTax) {
        this.individualIncomeTax = individualIncomeTax;
    }

    public BigDecimal getOtherExpenditure() {
        return otherExpenditure;
    }

    public void setOtherExpenditure(BigDecimal otherExpenditure) {
        this.otherExpenditure = otherExpenditure;
    }

    public BigDecimal getGrossIncome() {
        return grossIncome;
    }

    public void setGrossIncome(BigDecimal grossIncome) {
        this.grossIncome = grossIncome;
    }

    public BigDecimal getMonthlyAverageAvailable() {
        return monthlyAverageAvailable;
    }

    public void setMonthlyAverageAvailable(BigDecimal monthlyAverageAvailable) {
        this.monthlyAverageAvailable = monthlyAverageAvailable;
    }
}
